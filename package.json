{
  "name": "@oclif/plugin-help",
  "description": "Standard help for oclif.",
  "version": "6.0.12",
  "author": "Salesforce",
  "bugs": "https://github.com/oclif/plugin-help/issues",
  "dependencies": {
    "@oclif/core": "^3.18.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.1.12",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18",
    "chai": "^4.4.1",
    "commitlint": "^17.8.1",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.39",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "oclif": "^4.4.2",
    "prettier": "^3.2.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": "./lib/index.js",
  "files": [
    "oclif.manifest.json",
    "/lib",
    "/oclif.lock"
  ],
  "homepage": "https://github.com/oclif/plugin-help",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif",
    "flexibleTaxonomy": true,
    "topicSeparator": " "
  },
  "repository": "oclif/plugin-help",
  "scripts": {
    "build": "shx rm -rf lib && tsc",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm oclif.manifest.json oclif.lock",
    "posttest": "yarn lint",
    "prepack": "yarn run build && oclif manifest . && oclif lock",
    "prepare": "husky install && yarn build",
    "pretest": "yarn build && tsc --noEmit",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "type": "module",
  "types": "./lib/index.d.ts"
}
